{
    "name": "Bosco",
    "description": "Whatsapp bot made by pepesir using baileys",
    "repository": "https://github.com/pepesir/Bosco",
    "logo": "https://telegra.ph/file/d8a70dbcdfa75bafc6da0.jpg",
    "keywords": ["whatsbot", "whatsapp-bot", "whatsapp","bot","bosco","bosco-md","pepesir","md"],
    "stack": "container",
    "env": {
      "BOT_NAME": {
        "description": "Bot name.",
        "required": false,
        "value": "Bosco"
      },
      "BOT_THUMB": {
        "description": "Bot thumb link.",
        "required": false,
        "value": "https://telegra.ph/file/8fd47f9ccb7c39008b998.jpg"
      },
      "BOT_FOOTER": {
        "description": "Footer text.",
        "required": false,
        "value": " 「 ʙᴏꜱᴄᴏ ʙᴏᴛ 」\nᴄʀᴇᴀᴛᴇᴅ ʙy ᎮᏋᎮᏋ Sɪʀ ×፝֟͜×"
      },
      "BOT_VIDEO": {
        "description": "any video link.",
        "required": false,
        "value": "https://telegra.ph/file/a209db546105ed9447d2a.mp4"
      },
      "ALIVE_TEXT": {
        "description": "Alive text.",
        "required": false,
        "value": "Bot is alive"
      },
      "OWNER_LINK": {
        "description": "Provide any link.",
        "required": false,
        "value": "https://instagram.com/pepe.sir_?"
      },
      "HEROKU_APP_NAME": {
        "description": "Heroku app name, same as above entered",
        "required": true
      },
      "HEROKU_API_KEY": {
        "description": "Heroku account api key, https://dashboard.heroku.com/account",
        "required": true
      },
      "PREFIX": {
        "description": "Bot handler/prefix.",
        "required": false,
        "value": "."
      },
      "MODE": {
        "description": "Private or public",
        "required": true,
        "value":"public"
      },
      "SESSION_ID": {
        "description": "session id",
        "required": true
      }
    },
      "addons": [{
        "plan": "heroku-postgresql"
      }],
      "buildpacks": [{
        "url": "heroku-community/apt"
        }]
    }
  
